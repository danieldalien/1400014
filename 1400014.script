################################
# Check home pos
################################
def check_home():
  here = get_actual_tcp_pose()
  if point_dist(here,home) > 0.05:
    status_ur = UR_READY_NOHOME
    return False
  else:
    status_ur = UR_READY_HOME
    return True
  end
end

